<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Registartion Page</title>
    <!-- Latest compiled and minified CSS -->
    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/4.5.2/css/bootstrap.min.css">

    <!-- jQuery library -->
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.5.1/jquery.min.js"></script>

    <!-- Popper JS -->
    <script src="https://cdnjs.cloudflare.com/ajax/libs/popper.js/1.16.0/umd/popper.min.js"></script>

    <!-- Latest compiled JavaScript -->
    <script src="https://maxcdn.bootstrapcdn.com/bootstrap/4.5.2/js/bootstrap.min.js"></script>
    <link rel="stylesheet" href="https://use.fontawesome.com/releases/v5.6.3/css/all.css" integrity="sha384-UHRtZLI+pbxtHCWp1t77Bi1L4ZtiqrqD80Kn4Z8NTSRyMA2Fd33n5dQ8lWUE00s/" crossorigin="anonymous">



    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap@4.6.0/dist/css/bootstrap.min.css">
    <script src="https://code.jquery.com/ry-3jque.5.1.min.js"></script>
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@4.6.0/dist/js/bootstrap.bundle.min.js"></script>

    <style type="text/css">
        .node {
            border-style: solid;
        }
    </style>
</head>

<body>

    <section>
        <div class="container-fluid" >
            <div style="margin: 13%;">

                <div class="node" style="background-color: aquamarine;">
                    <div class="row" style="align-items: center; ">
                        <div class="col-md-2">
                        </div>
                        <div class="col-md-8">
                            <br>
                            <h1 class="mt-8 p-2 bg-secondary text-black rounded" style="align-items: center; text-align: center;">REGISTRATION</h1>
                            <br>
                            <% if (typeof error !=='undefined' && error.length> 0) { %>
                                <div class="container">
                                    <div class="alert alert-warning" style="text-align: center;">
                                        <%= error %> &nbsp; &nbsp; &nbsp;<a href="/" style="color:#8a6d3b;">&#10006;</a>
                                    </div>
                                </div>
                                <% } %>

                                    <form action="/signup" method="post" enctype="multipart/form-data">
                                        <div class="row">
                                            <div class="col-sm-6 col-lg-6 col-md-6">
                                                <label for="fname">First Name:</label>
                                                <input type="name" id="fname" name="fname" placeholder="Enter your first name" >
                                                <% if (typeof error1 !=='undefined' && error1.length> 0) { %>
                                                    <div style="color: red;">
                                                        <%= error1 %>
                                                    </div>
                                                    <% } %>
                                            </div>

                                        </div><br>
                                        <div class="row">
                                            <div class="col-sm-6 col-lg-6 col-md-6">
                                                <label for="mname">Middle Name:</label>
                                                <input type="name" id="mname" name="mname" placeholder="Enter your Middle name" >
                                                <% if (typeof error2 !=='undefined' && error2.length> 0) { %>
                                                    <div style="color: red;">
                                                        <%= error2 %>
                                                    </div>
                                                    <% } %>
                                            </div>

                                        </div><br>
                                        <div class="row">
                                            <div class="col-sm-6 col-lg-6 col-md-6">
                                                <label for="lname">Last Name:</label>
                                                <input type="name" id="lname" name="lname" placeholder="Enter your last name" >
                                                <% if (typeof error3 !=='undefined' && error3.length> 0) { %>
                                                    <div style="color: red;">
                                                        <%= error3%>
                                                    </div>
                                                    <% } %>
                                            </div>

                                        </div><br>
                                        <div class="row">
                                            <div class="col-md-9">
                                                <label for="gender">Gender:</label>
                                                <input type="radio" id="gender" name="gender" value="Male" > Male
                                                <input type="radio" id="gender" name="gender" value="Female" > Female
                                                <input type="radio" id="gender" name="gender" value="other" > Other

                                                <% if (typeof error4 !=='undefined' && error4.length> 0) { %>
                                                    <div style="color: red;">
                                                        <%= error4 %>
                                                    </div>
                                                    <% } %>
                                            </div>
                                            <div class="col-md-6"></div>
                                        </div><br>
                                        
                                        <div class="row">
                                            <div class="col-md-9">
                                                <label for="mobile">Mobile.No.:</label>
                                                <input type="text" id="mobile" name="mobile" placeholder="Enter your mobile number" >
                                            </div>
                                            <% if (typeof error6 !=='undefined' && error6.length> 0) { %>
                                                <div style="color: red;">
                                                    <%= error6 %>
                                                </div>
                                                <% } %>
                                                    <div class="col-md-6"></div>
                                        </div><br>

                                        <!-- <div class="row">
                                            <div class="col-md-9">
                                                <label for="Address" class="control-label">Address :</label>
                                                <input class="form-control" style="height: 70px;" type="text" name="Address">
                                            </div>
                                            <div class="col-md-6"></div>
                                        </div><br> -->

                                        <div class="row">
                                            <div class="col-md-9">
                                                <label for="city">City:</label>
                                                <select name="city">
                                           <option value=" " >Select City </option>
                                            <option value="Ahmedabad" >Ahmedabad</option>
                                            <option value="Surat">Surat</option>
                                            <option value="Rajkot">Rajkot</option>
                                            <option value="Junagadh">Junagadh </option>
                                            
                                        </select>
                                            </div>
                                            <% if (typeof error8 !=='undefined' && error8.length> 0) { %>
                                                <div style="color: red;">
                                                    <%= error8 %>
                                                </div>
                                                <% } %>
                                                    <div class="col-md-6"></div>
                                        </div><br>
                                        <div class="row">
                                            <div class="col-md-9">
                                                <label for="Email">Email:</label>
                                                <input type="Email" id="Email" name="Email" placeholder="Enter your email" >
                                                <% if (typeof error9!=='undefined' && error9.length> 0) { %>
                                                    <div style="color: red;">
                                                        <%= error9 %>
                                                    </div>
                                                    <% } %>
                                            </div>
                                            <div class="col-md-6"></div>
                                        </div><br>
                                        <div class="row">
                                            <div class="col-md-9">
                                                <label for="Password">User Password:</label>
                                                <input type="Password" id="Password" name="Password" placeholder="Enter your password" >
                                                <% if (typeof error10!=='undefined' && error10.length> 0) { %>
                                                    <div style="color: red;">
                                                        <%= error10 %>
                                                    </div>
                                                    <% } %>
                                            </div>
                                            <div class="col-md-6"></div>
                                        </div><br>

                                        <div class="row">
                                            <div class="col-md-9">
                                                <label for="ConfirmPassword">Confirm Password:</label>
                                                <input type="Password" id="ConfirmPassword" name="ConfirmPassword" placeholder="Enter your ConfirmPassword">
                                                <% if (typeof error11!=='undefined' && error11.length> 0) { %>
                                                    <div style="color: red;">
                                                        <%= error11 %>
                                                    </div>
                                                    <% } %>
                                            </div>
                                            <div class="col-md-6"></div>
                                        </div><br>
 
                                        <div class="row">
                                            <div class="col-md-9">
                                                <label for="hobby">Hobby:</label>
                                                <input type="checkbox" id="hobby" name="hobby" value="music" > Music
                                                <input type="checkbox" id="hobby" name="hobby" value="dance" > Dancing
                                                <input type="checkbox" id="hobby" name="hobby" value="drive" > Driving
                                                <input type="checkbox" id="hobby" name="hobby" value="game" > Gaming

                                            </div>

                                            <% if (typeof error5 !=='undefined' && error5.length> 0) { %>
                                                <div style="color: red;">
                                                    <%= error5 %>
                                                </div>
                                                <% } %>
                                                    <div class="col-md-6"></div>
                                        </div><br>

                                        <div class="row">
                                            <div class="col-md-9">
                                                <label for="Image" class="control-label">Upload Image:</label>
                                                <input class="form-control" style="height: 70px;
                                        padding: 20px;" type="file" name="Image">
                                            </div>
                                            <div class="col-md-6"></div>
                                        </div><br>


                                        <div class="row">
                                            <div class="col-md-9">
                                                <input class="btn btn-secondary" style="margin: 0% 0% 5% 60%; align-items: center; " type="submit" value="Submit">
                                            </div>
                                            <div class="col-md-6"></div>
                                        </div><br>
                                    </form>
                        </div>
                        <div class="col-md-2">
                        </div>

                    </div>
                </div>
            </div>

        </div>
    </section>


</body>

</html>